  "repository": {
    "type": "git",
    "url": "https://github.com/javascript-in-jena/webseite"
  },
  "bugs": {
    "url": "https://github.com/javascript-in-jena/meta/issues"
  },
  "dependencies": {
    "@datenknoten/ts-ssg": "0.7.2",
    "@datenknoten/tslint-config": "0.2.0",
    "chokidar-cli": "1.2.2",
    "http-server": "0.11.1",
    "moment": "2.24.0",
    "netlify-cli": "2.8.1",
    "node-sass": "4.11.0",
    "ts-node": "8.0.2",
    "tslint-language-service": "0.9.9",
    "typescript": "3.3.3333"
  }
